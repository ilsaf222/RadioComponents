@model RadioComponents.Models.ListInfoCategoryViewModel<RadioComponents.Models.CategoryViewModels.Capacitor.СapacitorCategoryViewModel>

@{
    ViewData["Title"] = "Info";
}

<h2>Категория</h2>
<div class="row">
    <div class="col-md-10">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

        <div class="form-group">
            <label asp-for="Type" class="control-label"></label>
            <input type="text" asp-for="Type" disabled />
        </div>
        <div class="form-group">
            <label asp-for="Name" class="control-label"></label>
            <input type="text" asp-for="Name" disabled />
        </div>
        <div class="form-group">
            <label asp-for="Configuration.WorkVolt" class="control-label"></label>
            <input type="number" asp-for="Configuration.WorkVolt" disabled />
        </div>
        <div class="form-group">
            <label asp-for="Configuration.RatedCapacity" class="control-label"></label>
            <input type="number" asp-for="Configuration.RatedCapacity" disabled />
        </div>
        <div class="form-group">
            <label asp-for="Configuration.RatedCapacityTolerance" class="control-label"></label>
            <input type="number" asp-for="Configuration.RatedCapacityTolerance" disabled />
        </div>
        <div class="form-group">
            <label asp-for="Configuration.LossTangent" class="control-label"></label>
            <input type="number" asp-for="Configuration.LossTangent" disabled />
        </div>
        <div class="form-group">
            <label asp-for="Configuration.ConclusionsAndCase" class="control-label"></label>
            <input type="text" asp-for="Configuration.ConclusionsAndCase" disabled />
        </div>

        <div class="form-group">
            <label asp-for="Configuration.MinimumWorkTemp" class="control-label"></label>
            <input type="number" asp-for="Configuration.MinimumWorkTemp" disabled />
        </div>
        <div class="form-group">
            <label asp-for="Configuration.MaximumWorkTemp" class="control-label"></label>
            <input type="number" asp-for="Configuration.MaximumWorkTemp" disabled />
        </div>
        <div class="form-group">
            <label asp-for="Configuration.CaseDiameter" class="control-label"></label>
            <input type="number" asp-for="Configuration.CaseDiameter" disabled />
        </div>
        <div class="form-group">
            <label asp-for="Configuration.CaseLength" class="control-label"></label>
            <input type="number" asp-for="Configuration.CaseLength" disabled />
        </div>
        <div class="form-group">
            <label asp-for="Configuration.Weight" class="control-label"></label>
            <input type="number" asp-for="Configuration.Weight" disabled />
        </div>
        <div class="form-group">
            <input type="submit" value="Создать" class="btn btn-primary" />
        </div>
    </div>
</div>

<div>
    <a asp-action="List">Назад</a>
</div>


