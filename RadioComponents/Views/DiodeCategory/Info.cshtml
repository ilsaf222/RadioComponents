@model RadioComponents.Models.ListInfoCategoryViewModel<RadioComponents.Models.CategoryViewModels.Diode.DiodeCategoryViewModel>
@{
    ViewData["Title"] = "Info";
}

<h2>Категория</h2>
<div class="row">
    <div class="col-md-10">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group">
                <label asp-for="Type" class="control-label"></label>
                <input type="text" asp-for="Type" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input type="text" asp-for="Name" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.NumberDiodesCase" class="control-label"></label>
                <input type="number" asp-for="Configuration.NumberDiodesCase" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.DiodeConfiguration" class="control-label"></label>
                <input type="text" asp-for="Configuration.DiodeConfiguration" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.MaxDCReverseVolt" class="control-label"></label>
                <input type="number" asp-for="Configuration.MaxDCReverseVolt" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.MaximumForwardCurrent" class="control-label"></label>
                <input type="number" asp-for="Configuration.MaximumForwardCurrent" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.MaxForwardVoltAt25" class="control-label"></label>
                <input type="text" asp-for="Configuration.MaxForwardVoltAt25" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.MaxReverseCurrentAt25" class="control-label"></label>
                <input type="text" asp-for="Configuration.MaxReverseCurrentAt25" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.CapacityAtVr" class="control-label"></label>
                <input type="text" asp-for="Configuration.CapacityAtVr" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.OperatingTempPNJunction" class="control-label"></label>
                <input type="text" asp-for="Configuration.OperatingTempPNJunction" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.Case" class="control-label"></label>
                <input type="text" asp-for="Configuration.Case" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.Weight" class="control-label"></label>
                <input type="number" asp-for="Configuration.Weight" disabled/>
            </div>
    </div>
</div>

<div>
    <a asp-action="List">Назад</a>
</div>

