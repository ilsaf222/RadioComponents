@model RadioComponents.Models.ListInfoCategoryViewModel<RadioComponents.Models.CategoryViewModels.Resistor.ResistorCategoryViewModel>
@{
    ViewData["Title"] = "Info";
}

<h2>Категория</h2>
<div class="row">
    <div class="col-md-10">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group">
                <label asp-for="Type" class="control-label"></label>
                <input type="text" asp-for="Type" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input type="text" asp-for="Name" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.ResType" class="control-label"></label>
                <input type="text" asp-for="Configuration.ResType" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.Resistance" class="control-label"></label>
                <input type="number" asp-for="Configuration.Resistance" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.Accuracy" class="control-label"></label>
                <input type="number" asp-for="Configuration.Accuracy" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.Power" class="control-label"></label>
                <input type="number" asp-for="Configuration.Power" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.MaxVoltage" class="control-label"></label>
                <input type="number" asp-for="Configuration.MaxVoltage" disabled/>
            </div>

            <div class="form-group">
                <label asp-for="Configuration.MinimumWorkTemp" class="control-label"></label>
                <input type="number" asp-for="Configuration.MinimumWorkTemp" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.MaximumWorkTemp" class="control-label"></label>
                <input type="number" asp-for="Configuration.MinimumWorkTemp" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.CaseDiameter" class="control-label"></label>
                <input type="number" asp-for="Configuration.CaseDiameter" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.CaseLength" class="control-label"></label>
                <input type="number" asp-for="Configuration.CaseLength" disabled/>
            </div>
            <div class="form-group">
                <label asp-for="Configuration.Weight" class="control-label"></label>
                <input type="number" asp-for="Configuration.Weight" disabled/>
            </div>
    </div>
</div>

<div>
    <a asp-action="List">Назад</a>
</div>

